                        commodo quis tortor et, congue volutpat velit.
                    </p>
                </div>
            </div>
            <!-- About To School -->
        </div>
    </div>
</section>
<!-- About The School -->
<!-- Gallery Section -->
<section class="gallery-holder white-bg">

    <!-- Main Heading -->
    <div class="main-heading-holder section-padding-top">
        <div class="main-heading">
            <h2>School <strong>Products</strong><span>our best services for your children</span></h2>
        </div>
        <!-- Filter Tags List -->
        <div class="filter-tags-holder">
            <ul id="tg-filterbale-nav" class="option-set">
                <li><a class="selected" data-filter="*" href="#">Hamısı</a></li>
                @foreach (edu_categories cat in Model.EduCategory)
                {
                    <li>
                        <a data-filter=".@cat.id" href="#">@cat.name</a>
                    </li>
                }
            </ul>
        </div>
        <!-- Filter Tags List -->
    </div>
    <!-- Main Heading -->
    <!-- Gallery Grid -->
    <div class="container">
        <div class="row">
            <div id="filter-masonry" class="gallery-masonry section-padding-top">
                @foreach (education edu in Model.Education)
                {                    
                    <div class="masonry-grid col-sm-4 col-xs-6 @edu.edu_cat_id r-full-width">
                        <figure class="gallery-figure">
                            <img src="~/Uploads/@edu.photo" alt="@edu.name">
                            <figcaption class="gallery-hover">
                                <div class="tc-display-table">
                                    <div class="tc-display-table-cell">
                                        <a class="fa fa-search" href="~/Uploads/@edu.photo" data-rel="prettyPhoto[gallery]"></a>
                                    </div>
                                </div>
                            </figcaption>
                        </figure>
                    </div>
                }
            </div>
        </div>
    </div>
    <!-- Gallery Grid -->

</section>
<!-- Gallery Section -->
